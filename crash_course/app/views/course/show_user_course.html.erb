
<legend>My Teachings</legend>

<table class="table table-condensed">
	<thead>
		<tr>
			<th>Course name</th>
			<th>Status</th>
		</tr>
	</thead>
	<tbody id="teaching">
		<%@user_teachings.each do |teaching|%>
			<tr>
				<td><%=teaching.title%></td>
				<%if(teaching.start_time >= DateTime.now) %>
					<td>On Time</td>
				<% else %>
					<td>Expired</td>
				<% end %>  
				
			</tr>	
		<% end %>
	</tbody>
</table>

